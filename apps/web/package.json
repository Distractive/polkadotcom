{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "stink-studios",
    "url": "https://stinkstudios.com"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:turbo": "next dev --turbo",
    "build": "BUILD_FLAG=true next build",
    "build:static": "rm -rf .next out && BUILD_FLAG=true NEXT_TELEMETRY_DISABLED=1 NEXT_DISABLE_VERCEL_ANALYTICS=1 NEXT_PUBLIC_BUILD_TYPE=static NODE_ENV=production next build",
    "run:static": "npx serve out",
    "static": "pnpm build:static && pnpm run:static",
    "start": "next start",
    "test": "SKIP_ENV_VALIDATION=true vitest",
    "test:ui": "SKIP_ENV_VALIDATION=true vitest --ui",
    "test:run": "SKIP_ENV_VALIDATION=true vitest run",
    "typecheck": "tsc --noEmit",
    "preview": "next build && next start",
    "export": "next build && next export",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs",
    "preplaywright:test": "echo '\\033[33mPlease check if playwright dependencies are installed. If not, run `pnpm exec playwright install chromium --with-deps`\\033[0m'",
    "playwright:test": "NODE_ENV=test playwright test",
    "algolia:migrate": "tsx scripts/algolia-migrate.ts"
  },
  "dependencies": {
    "@next/third-parties": "^14.2.3",
    "@portabletext/react": "^3.0.18",
    "@radix-ui/react-slot": "^1.0.2",
    "@sanity/code-input": "^4.1.4",
    "@sanity/preview-url-secret": "^1.6.17",
    "@sanity/ui": "^2.1.12",
    "@sanity/vision": "^3.37.2",
    "@shared/ui": "workspace:*",
    "@splinetool/react-spline": "^4.0.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@vercel/analytics": "^1.3.1",
    "algoliasearch": "^5.20.3",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.6.0",
    "groqd": "^0.15.11",
    "groqd-playground": "^0.0.19",
    "next": "^14.2.21",
    "next-sanity": "^9.0.2",
    "next-sitemap": "^4.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-instantsearch": "^7.15.3",
    "react-player": "^2.16.0",
    "sanity": "^3.46.1",
    "sanity-plugin-media": "^2.3.2",
    "sanity-plugin-vercel-deploy": "^3.3.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@playwright/test": "^1.50.1",
    "@shared/tailwind-config": "workspace:*",
    "@shared/ts-config": "workspace:*",
    "@storybook/addon-a11y": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/nextjs": "^8.1.5",
    "@storybook/react": "^8.0.5",
    "@storybook/test": "^8.1.5",
    "@types/node": "^20.12.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "storybook": "^8.0.5",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "5.8.2"
  }
}
